{
  "name": "@securetrading/js-payments-card",
  "version": "1.0.22",
  "description": "TypeScript Payment Card",
  "private": false,
  "browserslist": [
    "> 0.2% and last 2 versions",
    "not dead",
    "IE 10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:automated": "webpack --config webpack.test.js",
    "start:automated": "webpack-dev-server --color --progress --config webpack.test.js",
    "start": "webpack-dev-server --color --progress --config webpack.dev.js",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "prod": "npm run build && npm run copy",
    "snyk": "snyk test --org=secure-trading",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "copy": "copyfiles --flat ./dist/stcard.js ./dist/stcardstyle.css ./",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing &&",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./src/core/translations/$npm_package_config_locale.json -t ./translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./translations/$npm_package_config_locale.po -t ./src/core/translations/$npm_package_config_locale.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecureTrading/js-payments-card.git"
  },
  "resolutions": {
    "ajv": "^6.12.0",
    "minimist": "^1.2.5"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SecureTrading/js-payments-card/issues"
  },
  "homepage": "https://github.com/SecureTrading/js-payments-card#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-classes": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@types/jest-each": "^24.3.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "i18next-conv": "^10.0.2",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
    "jest-each": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-get-type": "^26.0.0",
    "live-server": "^1.2.1",
    "local-web-server": "^4.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.1.1",
    "prettier": "2.0.5",
    "sass-loader": "^9.0.2",
    "snyk": "^1.361.3",
    "source-map-loader": "^1.0.1",
    "stylelint": "^13.6.1",
    "stylelint-webpack-plugin": "^2.1.0",
    "ts-jest": "^26.1.1",
    "ts-loader": "^8.0.0",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@securetrading/ts-iin-lookup": "^0.0.8",
