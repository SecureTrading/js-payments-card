{
  "name": "@securetrading/js-payments-card",
  "version": "1.0.6",
  "description": "TypeScript Payment Card",
  "private": false,
  "browserslist": [
    "> 0.2% and last 2 versions",
    "not dead",
    "IE 10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:automated": "webpack --config webpack.test.js",
    "start:automated": "webpack-dev-server --color --progress --config webpack.test.js",
    "start": "webpack-dev-server --color --progress --config webpack.dev.js",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "prod": "npm run build && npm run copy",
    "snyk": "snyk test --org=secure-trading",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "copy": "copy-files-from-to",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing &&",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./src/core/translations/$npm_package_config_locale.json -t ./translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s ./translations/$npm_package_config_locale.po -t ./src/core/translations/$npm_package_config_locale.json"
  },
  "copyFiles": [
    {
      "from": "dist/stcard.js",
      "to": "stcard.js"
    },
    {
      "from": "dist/stcardstyle.css",
      "to": "stcardstyle.css"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecureTrading/js-payments-card.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SecureTrading/js-payments-card/issues"
  },
  "homepage": "https://github.com/SecureTrading/js-payments-card#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.19",
    "@types/jest-each": "^24.3.0",
    "@types/jest-matchers": "^20.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.0",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "i18next-conv": "^9.1.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-each": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-get-type": "^24.9.0",
    "live-server": "^1.2.1",
    "local-web-server": "^3.0.7",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^2.0.0",
    "prettier": "1.19.1",
    "sass-loader": "^8.0.1",
    "snyk": "^1.237.1",
    "source-map-loader": "^0.2.4",
    "stylelint": "^13.0.0",
    "stylelint-webpack-plugin": "^1.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.16.2",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.6.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/runtime": "^7.6.3",
    "@types/joi": "^14.3.3",
    "@types/promise-polyfill": "^6.0.3",
    "copy-files-from-to": "^2.1.0",
    "es6-promise-promise": "^1.0.0",
    "i18next": "^19.0.0",
    "joi": "^14.3.1",
    "joi-browser": "^13.4.0",
    "location-origin": "^1.1.4",
    "promise-polyfill": "8.1.3",
    "resolve-url-loader": "^3.1.0",
    "url-polyfill": "^1.1.7",
    "validate": "^5.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "snyk": true
}
